{
  "name": "konshuu",
  "version": "0.0.1",
  "description": "Weekly planner for your work and personal todos.",
  "private": true,
  "type": "module",
  "main": "src/server/konshuu-server.ts",
  "scripts": {
    "build:prod": "npm run build-server:prod && npm run build-static:prod",
    "build-static:prod": "./static-prebuild.sh && vite build",
    "build-static:dev": "./static-prebuild.sh && vite",
    "build-server:prod": "tsc",
    "build-server:dev": "tsc --watch",
    "lint-server:prod": "eslint 'src/server/**/*.ts' && tsc --noEmit",
    "lint-static:dev": "svelte-check --watch",
    "lint-static:prod": "eslint 'src/static/**/*.{js,ts,svelte}' && svelte-check",
    "dev": "nodemon --ignore public/ --inspect=0.0.0.0 dist/server/app.js",
    "style:write": "prettier --write .",
    "style:check": "prettier --check . --loglevel warn",
    "test": "npm run style:check && npm run lint-static:prod && npm run lint-server:prod",
    "prepare": "husky install"
  },
  "author": "sheodox",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@prisma/client": "^4.0.0",
    "bcrypt": "^5.0.1",
    "connect-redis": "^6.0.0",
    "date-fns": "^2.28.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.1.23",
    "onaji": "^0.1.3",
    "page": "^1.11.6",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "prom-client": "^14.0.1",
    "pug": "^3.0.2",
    "redis": "^3.1.2",
    "sass": "^1.32.5",
    "serialize-javascript": "^5.0.1",
    "sheodox-ui": "^0.17.9",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.3.2",
    "svelte": "^3.44.1",
    "ts-node": "^10.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "0.0.16",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/node": "^14.14.31",
    "@types/page": "^1.11.5",
    "@types/passport": "^1.0.10",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.29",
    "@types/serialize-javascript": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^7.0.0",
    "nodemon": "^2.0.4",
    "prettier": "2.4.1",
    "prettier-plugin-svelte": "2.4.0",
    "prisma": "^4.0.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^2.6.13"
  }
}
